; ** Convertir GO IN into ENTER
GO   _
 PREP IN
 EQ 34 255
 LET 33 &&_voc_ENTER

_    _
 PREP IN
 EQ 33 255  ; No verb
 EQ 34 255  ; No noun
 LET 33 &&_voc_ENTER
 LET 46 255

; ** Convert LOOK INTO into EXAMINE

LOOK _
 NOTEQ 34 255          
 PREP INTO
 LET 33 &&_voc_EX

; ** Convert LOOK "SOMEHTING" en EX "SOMETHING"

LOOK _
 NOTEQ 34 255
 LET 33 &&_voc_EX
 
LOOK _
 BNOTZERO 12 1 
 LET 33 &&_voc_EX
  
 
; ** Convert REGISTER in EX INTO

REGISTER _
 LET 33 &&_voc_EX
 LET 43 &&_voc_INTO

