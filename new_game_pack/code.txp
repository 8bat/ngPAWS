 LET 33 &&_voc_ENTER
 LET 46 255

; ** Convert LOOK INTO into EXAMINE

LOOK _
 NOTEQ 34 255          
 PREP INTO
 LET 33 &&_voc_EX

; ** Convert LOOK "SOMEHTING" en EX "SOMETHING"

LOOK _
 NOTEQ 34 255
 LET 33 &&_voc_EX
 
LOOK _
 BNOTZERO 12 1 
 LET 33 &&_voc_EX
  
 
